{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "locations",
      "options": [
        "castro-street",
        "eureka",
        "home",
        "work"
      ]
    },
    {
      "id": "meals",
      "options": [
        "breakfast",
        "lunch",
        "dinner"
      ]
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "dateFormat": "yyyy-MM-dd'T'HH:mm:ss.SSSXXX"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Locations",
          "name": "locations",
          "type": "taxonomy",
          "taxonomyId": "locations"
        },
        {
          "title": "Meals",
          "name": "meals",
          "type": "taxonomy",
          "taxonomyId": "meals"
        },
        {
          "title": "Rating",
          "name": "rating",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "post",
      "path": "[[workspace]]/content/post"
    }
  ]
}